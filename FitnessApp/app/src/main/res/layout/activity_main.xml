<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/a1"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/btnmusic"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="168dp"
        android:layout_marginRight="168dp"
        android:layout_marginBottom="0dp"
        android:background="?android:selectableItemBackground"

        android:contentDescription="TODO"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/music" />

    <ImageButton
        android:id="@+id/btnfood"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="246dp"
        android:layout_marginRight="246dp"
        android:layout_marginBottom="0dp"
        android:background="?android:selectableItemBackground"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/foodie" />

    <ImageButton

        android:id="@+id/btnworkout"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="90dp"
        android:layout_marginRight="90dp"
        android:layout_marginBottom="0dp"
        android:background="?android:selectableItemBackground"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/workoutt" />

    <ImageButton
        android:id="@+id/btnplanner"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginBottom="0dp"
        android:background="?android:selectableItemBackground"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/planner" />

    <ImageButton
        android:id="@+id/btnhome"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="325dp"
        android:layout_marginRight="325dp"
        android:layout_marginBottom="0dp"
        android:background="?android:selectableItemBackground"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/homes" />

    <ImageButton
        android:id="@+id/btnhamburger"
        android:layout_width="78dp"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="10dp"
        android:background="?android:selectableItemBackground"
        android:scaleType="fitCenter"
        android:tint="#FF7F50"
        app:srcCompat="@drawable/ham1" />

</RelativeLayout>